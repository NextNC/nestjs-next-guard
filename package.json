{
  "name": "@nextnm/nestjs-next-guard",
  "version": "2.2.0",
  "description": "NestJS RBAC Guard that also checks models ownership",
  "author": "Nuno Carvalh√£o <nuno.carvalhao@gmail.com>",
  "license": "MIT",
  "readmeFilename": "README.md",
  "main": "dist/index.js",
  "files": [
    "dist/**/*",
    "*.md"
  ],
  "scripts": {
    "start:dev": "tsc -w",
    "build": "tsc",
    "prepare": "npm run build",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "keywords": [
    "nestjs",
    "nextnm",
    "guard",
    "ownership",
    "RBAC",
    "Mongoose"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/NextNC/nestjs-next-guard"
  },
  "bugs": "https://github.com/NextNC/nestjs-next-guard",
  "peerDependencies": {
    "@nestjs/common": "^9.2.0",
    "rxjs": "^7.5.5",
    "reflect-metadata": "^0.1.13",
    "mongoose": "^6.10.1"
  },
  "dependencies": {
    "@nestjs/mongoose": "^9.2.1",
    "mongoose": "^6.10.1",
    "redis": "^3.1.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.5",
    "util": "^0.12.5"
  },
  "devDependencies": {
    "@nestjs/common": "^9.2.0",
    "@nestjs/core": "^9.2.0",
    "@types/redis": "^2.8.28",
    "@nestjs/platform-express": "^9.2.0",
    "@nestjs/testing": "9.2.0",
    "@types/express": "4.17.5",
    "@types/jest": "25.2.1",
    "@types/node": "^18.0.0",
    "@types/supertest": "^2.0.8",
    "jest": "25.3.0",
    "prettier": "2.0.4",
    "supertest": "^4.0.2",
    "ts-jest": "25.3.1",
    "ts-node": "10.4.0",
    "tsc-watch": "4.2.3",
    "tsconfig-paths": "3.9.0",
    "tslint": "6.1.1",
    "typescript": "4.5.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
